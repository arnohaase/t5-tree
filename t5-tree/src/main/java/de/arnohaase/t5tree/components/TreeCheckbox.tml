<t:container xmlns="http://www.w3.org/1999/xhtml"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
      xmlns:p="tapestry:parameter"
      >

  <span class="tree-checkbox ${checkboxClass}">
    <t:checkbox
      t:id="check" 
      value="checked"
      class="${hierarchyClasses}" 
      onclick="t5tree.handleCheckBoxChange(this);${checkboxOnClick}" />
  </span>
      
</t:container>     
